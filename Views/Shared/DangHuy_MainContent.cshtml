			<div class="content">
				<!-- Begin New Product -->
				<main>
					@RenderBody()
				</main>
				<!-- End New Product -->
			</div>
		</div>
		<!-- End Main -->
	</div>
	<!-- End Wrap -->
	<!-- Begin Footer -->
	<div class="footer">
		<partial name="DangHuy_Footer" />
	</div>
	<!-- End Footer -->
</body>

</html>
